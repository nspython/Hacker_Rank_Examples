   "source": [
    "import itertools\n",
    "\n",
    "s, k = input().split()\n",
    "\n",
    "for j in range(1,int(k)+1):\n",
    "    for i in list(itertools.combinations(sorted(s),j)):\n",
    "        print(\"\".join(i))\n"
   ]
